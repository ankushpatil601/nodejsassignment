pipeline{

agent any 

stages{
       stage("Cloning Repository"){
              steps{
                  git 'https://github.com/ankushpatil601/nodejsassignment.git'
              }
      }
      stage("Install Dependencies"){
            steps{
              sh 'npm install'
            }
      }
      stage("Testing"){
            steps{
              sh 'npm test'
            }
      }
}

}
